# Local Development Environment
# Use this for testing with Docker Compose instead of Supabase cloud

SUPABASE_URL=http://localhost:3000
SUPABASE_ANON_KEY=local-development-key
SUPABASE_SERVICE_ROLE_KEY=local-service-role-key

# Local PostgreSQL (from Docker Compose)
SUPABASE_DB_URL=postgres://postgres:postgres@localhost:5432/postgres?sslmode=disable

# Local Redis (from Docker Compose)
REDIS_URL=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Application Settings
APP_PORT=8080
LOG_LEVEL=info
